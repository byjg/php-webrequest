{
  "name": "byjg/webrequest",
  "description": "A lightweight PSR-7 implementation and highly customized CURL wrapper for making REST calls.",
  "authors": [
    {
      "name": "João Gilberto Magalhães",
      "email": "joao@byjg.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByJG\\WebRequest\\": "src/"
    }
  },
    "autoload-dev": {
      "psr-4": {
        "Test\\": "tests/"
      }
    },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.3 <8.6",
    "ext-curl": "*",
    "ext-json": "*",
    "byjg/uri": "^6.0",
    "psr/http-message": "^1.0|^2.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0|^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6|^11.5",
    "vimeo/psalm": "^5.9|^6.13",
    "ext-soap": "*"
  },
  "provide": {
    "psr/http-client-implementation": "1.0",
    "psr/http-message-implementation": "1.0",
    "psr/http-factory-implementation": "1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm --threads=1"
  },
  "license": "MIT"
}
