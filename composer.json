{
  "name": "byjg/webrequest",
  "description": "A lightweight and highly customized CURL wrapper for making RESt calls using PSR7 implementation and a wrapper for call dynamically SOAP requests.",
  "authors": [
    {
      "name": "João Gilberto Magalhães",
      "email": "joao@byjg.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByJG\\WebRequest\\": "src/"
    }
  },
    "autoload-dev": {
      "psr-4": {
        "Test\\": "tests/"
      }
    },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1 <8.5",
    "ext-curl": "*",
    "ext-json": "*",
    "byjg/uri": "^6.0",
    "psr/http-message": "^1.0|^2.0",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0|^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "vimeo/psalm": "^5.9|^6.2",
    "ext-soap": "*"
  },
  "provide": {
    "psr/http-client-implementation": "1.0",
    "psr/http-message-implementation": "1.0",
    "psr/http-factory-implementation": "1.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "psalm": "vendor/bin/psalm"
  },
  "license": "MIT"
}
